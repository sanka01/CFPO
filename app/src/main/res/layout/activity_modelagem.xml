<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Modelagem">
    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center"
            android:id="@+id/view2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Metodo"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/ReposicaoEstoque"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center"
            android:id="@+id/view"
            android:layout_marginTop="156dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/Metodo"
            app:layout_constraintVertical_bias="0.877"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"/>
    <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/Metodo"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="400dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Spinner
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:id="@+id/ReposicaoEstoque"
            tools:layout_editor_absoluteX="0dp" app:layout_constraintBottom_toTopOf="@+id/button"
            android:layout_marginBottom="232dp"/>
    <Button
            android:text="Registrar Perfil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.988"/>
    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center"
            android:id="@+id/view3"
            app:layout_constraintTop_toBottomOf="@+id/ReposicaoEstoque"
            android:layout_marginTop="24dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Spinner
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            tools:layout_editor_absoluteX="8dp" android:id="@+id/Caixatual"
            app:layout_constraintTop_toBottomOf="@+id/view3" android:layout_marginTop="24dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>